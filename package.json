    "@types/memory-fs": "^0.3.3",
    "babel-jest": "^27.0.6",
    "jest": "^27.0.6",
    "memory-fs": "^0.5.0",
    "release-it": "^14.11.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "webpack": "^4.0.1",
    "webpack5": "npm:webpack@^5.51.1"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "jest": {
